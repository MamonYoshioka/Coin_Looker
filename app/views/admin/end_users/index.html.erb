<div class="container">
  <h1>会員一覧</h1>
  <table class='table table-hover table-inverse'>
      <thead>
      <tr>
        <th>会員ID</th>
        <th>氏名</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
        <th></th>
      </tr>
    </thead>
    <tbody>

    <% @end_users.each do |end_user| %>
        <tr>
        <td><%= end_user.id %></td>
        <td><%= end_user.family_name + end_user.first_name %></td>
        <td><%= end_user.email %></td>
        <td>
        <% if end_user.is_deleted == false %>
        有効
        <% else %>
        退会
        <% end %>
        </td>
        <% end %>
        </tr>
    </tbody>
   </table>
    <%= paginate @end_users %>
</div>